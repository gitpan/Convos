<li class="nick-quit" data-event="nick-quit" data-network="<%= $network %>" data-target="" data-nick="<%= $nick %>">
  %= timestamp $timestamp
  <div class="content">
    <b class="nick"><%= $nick %></b> quit.
  % $message =~ s/^quit\W+//i; # Quit: Leaving
  % if($message) {
    <span><%= ucfirst $message %></span>
  % }
</li>
